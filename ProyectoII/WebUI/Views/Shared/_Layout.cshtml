<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - Cenfo Market</title>
    <!-- Bootstrap -->
    <link href="~/Content/LandingAssets/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/LandingAssets/css/styles.css?v=1.2" rel="stylesheet">
    <link href="~/Content/LandingAssets/css/queries.css?v=1.2" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/LandingAssets/css/flexslider.css?v=1.2" type="text/css">
    <link rel="stylesheet" href="~/Content/LandingAssets/css/animate.css" type="text/css">
    <link href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
    <script src="~/Scripts/View/bootstrap.min.js"></script>
    <script src="~/Scripts/View/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/View/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/View/ControlActions.js"></script>
    <link href="~/Content/Site.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <style>
        header ul li a {
            margin: 0 19px;
        }
    </style>
</head>
<body>

    <header class="navbar navbar-fixed-top">
        <div class = "flex-navbar">
            <div>
                <img class="logo-size" src="~/Content/LandingAssets/img/logoCanva.png" />
            </div>

            <div class="container">
                @*<div class="row">
                    <div class="pullcontainer">
                        <a href="#" id="pull"><i class="fa fa-bars fa-2x"></i></a>
                    </div>
                </div>*@
                <div class="row">
                    <ul class="nav-bar-invisible">
                        <li>@Html.ActionLink("Log in", "Login", "Dashboard", "Dashboard", new { @class = "nav-link scrollto" })</li>
                        <li class="dot">.</li>
                        <li>@Html.ActionLink("Sign In", "UserForm", "Form", "Form", new { @class = "nav-link scrollto" })</li>
                    </ul>

                    <div class="col-md-8  remove col-md-offset-2">
                        <nav>
                            <div class="logo-holder"></div>
                            <ul class="clearfix">
                                <li><a href="#cenfoMarketPlace">Cenfo-MarketPlace</a></li>
                                <li class="dot">.</li>
                                <li><a href="#team">key People</a></li>
                                <li class="dot">.</li>
                                <li>@Html.ActionLink("Log in", "Login", "Dashboard", "Dashboard", new { @class = "nav-link scrollto" })</li>
                                <li class="dot">.</li>
                                <li>@Html.ActionLink("Sign In", "UserForm", "Form", "Form", new { @class = "nav-link scrollto" })</li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        
    </header>

    @RenderBody()
    
    <footer id="contact">
        <div class="container">
            <div class="row">
                <div class="col-md-4 footer-leftcol">
                    <p>
                        <span class="bold-16p">Cenfotec</span><br>
                        San José<br>
                        Costa Rica - <a href="#">cenfomarket.info@gmail.com</a>
                    </p>
                </div>
                <div>
                    <p class="bold-16p footer-right-format"><strong>Designed by Brainware-Tech </strong></p>
                </div>
            </div>
        </div>
    </footer>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins)-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/Content/LandingAssets/js/bootstrap.min.js"></script>
    <script src="~/Content/LandingAssets/js/jquery.flexslider.js"></script>
    <script src="~/Content/LandingAssets/js/scripts.js"></script>
    <script src="~/Content/LandingAssets/js/modernizr.js"></script>
    <script src="~/Content/LandingAssets/js/waypoints.min.js"></script>
</body>


</html>



